<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="http://www.lod.lu/" xmlns:lod="http://www.lod.lu/">
  <xs:element name="LOD">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lod:content-description"/>
        <xs:element name="content-description" form="unqualified">
          <xs:complexType mixed="true">
            <xs:attribute name="lang" use="required" type="xs:NCName"/>
          </xs:complexType>
        </xs:element>
        <xs:element ref="lod:META"/>
        <xs:element maxOccurs="unbounded" ref="lod:ITEM"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="content-description">
    <xs:complexType mixed="true">
      <xs:attribute name="lang" use="required" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="ITEM">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lod:META"/>
        <xs:element ref="lod:ARTICLE"/>
        <xs:element minOccurs="0" ref="lod:AUDIO"/>
        <xs:choice minOccurs="0">
          <xs:element ref="lod:FLX-ADJ"/>
          <xs:element ref="lod:FLX-VRB"/>
        </xs:choice>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ARTICLE">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lod:ITEM-ADRESSE"/>
        <xs:element ref="lod:MICROSTRUCTURE"/>
        <xs:choice>
          <xs:element ref="lod:RESEAU-NON-SEMANTIQUE"/>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="lod:tag"/>
        </xs:choice>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ITEM-ADRESSE">
    <xs:complexType mixed="true">
      <xs:attribute name="ID-ITEM-ADRESSE" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="MICROSTRUCTURE">
    <xs:complexType>
      <xs:choice>
        <xs:element ref="lod:MS-TYPE-ADJ"/>
        <xs:element ref="lod:MS-TYPE-ADV"/>
        <xs:element ref="lod:MS-TYPE-ART"/>
        <xs:element ref="lod:MS-TYPE-CONJ"/>
        <xs:element ref="lod:MS-TYPE-ELEM-COMP"/>
        <xs:element ref="lod:MS-TYPE-PART"/>
        <xs:element ref="lod:MS-TYPE-PREP"/>
        <xs:element ref="lod:MS-TYPE-PREP-plus-ART"/>
        <xs:element ref="lod:MS-TYPE-PRON"/>
        <xs:element ref="lod:MS-TYPE-PRONADV"/>
        <xs:element ref="lod:MS-TYPE-SUBST"/>
        <xs:element maxOccurs="unbounded" ref="lod:MS-TYPE-VRB"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="MS-TYPE-ADJ">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lod:CAT-GRAM-ADJ"/>
        <xs:choice>
          <xs:element ref="lod:RENVOI-ADJ"/>
          <xs:element ref="lod:TRAITEMENT-LING-ADJ"/>
        </xs:choice>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="CAT-GRAM-ADJ">
    <xs:complexType>
      <xs:attribute name="C-G-ADJ" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="RENVOI-ADJ">
    <xs:complexType>
      <xs:choice>
        <xs:element ref="lod:VARIANTE-HOMOSEME"/>
        <xs:element ref="lod:VARIANTE-ORTHOGRAPHIQUE"/>
      </xs:choice>
      <xs:attribute name="REF-ID-ITEM-ADRESSE" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="TRAITEMENT-LING-ADJ">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lod:TRADUCTION-DOMINANTE"/>
        <xs:element maxOccurs="unbounded" ref="lod:UNITE-TRAD"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="MS-TYPE-ADV">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lod:CAT-GRAM-ADV"/>
        <xs:choice>
          <xs:element ref="lod:COMPOSITION"/>
          <xs:element ref="lod:RENVOI-ADV"/>
          <xs:element ref="lod:TRAITEMENT-LING-ADV"/>
        </xs:choice>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="CAT-GRAM-ADV">
    <xs:complexType>
      <xs:attribute name="HF" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="TRAITEMENT-LING-ADV">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lod:TRADUCTION-DOMINANTE"/>
        <xs:element maxOccurs="unbounded" ref="lod:UNITE-TRAD"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="MS-TYPE-ART">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lod:CAT-GRAM-ART"/>
        <xs:element maxOccurs="unbounded" ref="lod:TRAITEMENT-LING-ART"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="CAT-GRAM-ART">
    <xs:complexType>
      <xs:attribute name="TYPE-ART" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="TRAITEMENT-LING-ART">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lod:INFO-GRAM-ART"/>
        <xs:element ref="lod:TRADUCTION-DOMINANTE"/>
        <xs:element ref="lod:UNITE-TRAD"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="INFO-GRAM-ART">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lod:NOMBRE-ART"/>
        <xs:element ref="lod:GENRE-ART"/>
        <xs:element ref="lod:CAS-ART"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="NOMBRE-ART">
    <xs:complexType>
      <xs:attribute name="NB-ART" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="GENRE-ART">
    <xs:complexType>
      <xs:attribute name="GENRE-ART" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="CAS-ART">
    <xs:complexType>
      <xs:attribute name="CAS-ART" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="MS-TYPE-CONJ">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lod:CAT-GRAM-CONJ"/>
        <xs:choice>
          <xs:element ref="lod:RENVOI-CONJ"/>
          <xs:element ref="lod:TRAITEMENT-LING-CONJ"/>
        </xs:choice>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="CAT-GRAM-CONJ">
    <xs:complexType/>
  </xs:element>
  <xs:element name="RENVOI-CONJ">
    <xs:complexType>
      <xs:choice>
        <xs:element ref="lod:VARIANTE-HOMOSEME"/>
        <xs:element ref="lod:VARIANTE-ORTHOGRAPHIQUE"/>
      </xs:choice>
      <xs:attribute name="REF-ID-ITEM-ADRESSE" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="TRAITEMENT-LING-CONJ">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lod:TRADUCTION-DOMINANTE"/>
        <xs:element maxOccurs="unbounded" ref="lod:UNITE-TRAD"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="MS-TYPE-ELEM-COMP">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lod:CAT-GRAM-ELEM-COMP"/>
        <xs:element ref="lod:UNITE-TRAD"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="CAT-GRAM-ELEM-COMP">
    <xs:complexType>
      <xs:attribute name="C-G-EC" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="MS-TYPE-PART">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lod:CAT-GRAM-PART"/>
        <xs:choice>
          <xs:element ref="lod:RENVOI-PART"/>
          <xs:element ref="lod:TRAITEMENT-LING-PART"/>
        </xs:choice>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="CAT-GRAM-PART">
    <xs:complexType/>
  </xs:element>
  <xs:element name="RENVOI-PART">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lod:VARIANTE-HOMOSEME"/>
      </xs:sequence>
      <xs:attribute name="REF-ID-ITEM-ADRESSE" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="TRAITEMENT-LING-PART">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lod:TRADUCTION-DOMINANTE"/>
        <xs:element ref="lod:UNITE-TRAD"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="MS-TYPE-PREP">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lod:CAT-GRAM-PREP"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="lod:TRAITEMENT-LING-PREP"/>
        <xs:element minOccurs="0" ref="lod:MARQUE-USAGE"/>
        <xs:element minOccurs="0" ref="lod:RENVOI-PREP"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="CAT-GRAM-PREP">
    <xs:complexType/>
  </xs:element>
  <xs:element name="TRAITEMENT-LING-PREP">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lod:CAS-ATTENDU"/>
        <xs:element ref="lod:SPATIOTEMP"/>
        <xs:element ref="lod:TRADUCTION-DOMINANTE"/>
        <xs:element maxOccurs="unbounded" ref="lod:UNITE-TRAD"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="CAS-ATTENDU">
    <xs:complexType>
      <xs:attribute name="CAS" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="SPATIOTEMP">
    <xs:complexType>
      <xs:attribute name="ST" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="MS-TYPE-PREP-plus-ART">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lod:CAT-GRAM-PREP-plus-ART"/>
        <xs:element ref="lod:COMPOSITION"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="CAT-GRAM-PREP-plus-ART">
    <xs:complexType/>
  </xs:element>
  <xs:element name="MS-TYPE-PRON">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lod:CAT-GRAM-PRON"/>
        <xs:choice>
          <xs:element ref="lod:RENVOI-PRON"/>
          <xs:element maxOccurs="unbounded" ref="lod:TRAITEMENT-LING-PRON"/>
        </xs:choice>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="CAT-GRAM-PRON">
    <xs:complexType>
      <xs:attribute name="TYPE-PRON" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="RENVOI-PRON">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lod:VARIANTE-HOMOSEME"/>
      </xs:sequence>
      <xs:attribute name="REF-ID-ITEM-ADRESSE" use="required" form="qualified" type="xs:NCName"/>
      <xs:attribute name="REFS-IDS-UNITES-DE-SENS" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="TRAITEMENT-LING-PRON">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="lod:INFO-GRAM-PRON"/>
        <xs:element minOccurs="0" ref="lod:COMPLEMENT-PRON"/>
        <xs:element ref="lod:TRADUCTION-DOMINANTE"/>
        <xs:element maxOccurs="unbounded" ref="lod:UNITE-TRAD"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="INFO-GRAM-PRON">
    <xs:complexType>
      <xs:sequence>
        <xs:choice minOccurs="0">
          <xs:element ref="lod:NOMBRE-PRON"/>
          <xs:element ref="lod:PERSONNE-PRON"/>
        </xs:choice>
        <xs:element minOccurs="0" ref="lod:GENRE-PRON"/>
        <xs:element minOccurs="0" ref="lod:CAS-PRON"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="NOMBRE-PRON">
    <xs:complexType>
      <xs:attribute name="NB-PRON" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="PERSONNE-PRON">
    <xs:complexType>
      <xs:attribute name="PERSONNE" use="required" form="qualified" type="xs:NMTOKEN"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="GENRE-PRON">
    <xs:complexType>
      <xs:attribute name="GENRE-PRON" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="CAS-PRON">
    <xs:complexType>
      <xs:attribute name="CAS-PRON" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="COMPLEMENT-PRON">
    <xs:complexType>
      <xs:attribute name="GENRE-PRON" use="required" form="qualified" type="xs:NCName"/>
      <xs:attribute name="NB-PRON" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="MS-TYPE-PRONADV">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lod:CAT-GRAM-PRONADV"/>
        <xs:choice>
          <xs:element ref="lod:COMPOSITION"/>
          <xs:element ref="lod:RENVOI-ADV"/>
        </xs:choice>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="CAT-GRAM-PRONADV">
    <xs:complexType>
      <xs:attribute name="C-G-PA" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="MS-TYPE-SUBST">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lod:CAT-GRAM-SUBST"/>
        <xs:element ref="lod:GENRE"/>
        <xs:element ref="lod:TYPE-SUBST"/>
        <xs:element ref="lod:PLURIEL"/>
        <xs:choice minOccurs="0">
          <xs:element ref="lod:TRAITEMENT-LING-SUBST"/>
          <xs:element ref="lod:RENVOI-ET-TRAIT-LING-SUPPL-SUBST"/>
        </xs:choice>
        <xs:element minOccurs="0" ref="lod:MARQUE-USAGE"/>
        <xs:element minOccurs="0" ref="lod:RENVOI-SUBST"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="CAT-GRAM-SUBST">
    <xs:complexType>
      <xs:attribute name="C-G-SUBST" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="GENRE">
    <xs:complexType>
      <xs:attribute name="GEN" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="TYPE-SUBST">
    <xs:complexType>
      <xs:choice>
        <xs:element ref="lod:DIM-MOT-SIMPLE-NON-TRAITE"/>
        <xs:element ref="lod:DIMINUTIF"/>
        <xs:element ref="lod:NOM-D-ELEMENT-HYDROLOGIQUE"/>
        <xs:element ref="lod:NOM-DE-LOCALITE"/>
        <xs:element ref="lod:NOM-DE-LOCALITE-NON-LUX"/>
        <xs:element ref="lod:NOM-DE-PAYS"/>
        <xs:element ref="lod:POLYLEX-FR"/>
        <xs:element ref="lod:SUBST-COMP-PREMIER-ELEM-NON-TRAITE"/>
        <xs:element ref="lod:SUBST-COMPOSE"/>
        <xs:element ref="lod:SUBST-SIMPLE"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="DIM-MOT-SIMPLE-NON-TRAITE">
    <xs:complexType/>
  </xs:element>
  <xs:element name="DIMINUTIF">
    <xs:complexType>
      <xs:attribute name="REF-ID-ARTICLE-DOMIN" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="NOM-D-ELEMENT-HYDROLOGIQUE">
    <xs:complexType/>
  </xs:element>
  <xs:element name="NOM-DE-LOCALITE">
    <xs:complexType/>
  </xs:element>
  <xs:element name="NOM-DE-LOCALITE-NON-LUX">
    <xs:complexType/>
  </xs:element>
  <xs:element name="NOM-DE-PAYS">
    <xs:complexType/>
  </xs:element>
  <xs:element name="POLYLEX-FR">
    <xs:complexType/>
  </xs:element>
  <xs:element name="SUBST-COMP-PREMIER-ELEM-NON-TRAITE">
    <xs:complexType/>
  </xs:element>
  <xs:element name="SUBST-COMPOSE">
    <xs:complexType>
      <xs:attribute name="REF-ID-ARTICLE-DOMIN" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="SUBST-SIMPLE">
    <xs:complexType/>
  </xs:element>
  <xs:element name="RENVOI-ET-TRAIT-LING-SUPPL-SUBST">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lod:RENVOI-SUBST"/>
        <xs:element ref="lod:TRAITEMENT-LING-SUBST"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="MS-TYPE-VRB">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lod:CAT-GRAM-VRB"/>
        <xs:choice>
          <xs:element ref="lod:NEMMEN-AM-INFINITIV"/>
          <xs:sequence>
            <xs:element ref="lod:VRB-AUXILIAIRE"/>
            <xs:element maxOccurs="unbounded" ref="lod:PARTICIPE-PASSE"/>
          </xs:sequence>
        </xs:choice>
        <xs:choice>
          <xs:element ref="lod:RENVOI-VRB"/>
          <xs:element maxOccurs="unbounded" ref="lod:TRAITEMENT-LING-VRB"/>
        </xs:choice>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="CAT-GRAM-VRB">
    <xs:complexType>
      <xs:attribute name="C-G-VRB" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="NEMMEN-AM-INFINITIV">
    <xs:complexType/>
  </xs:element>
  <xs:element name="VRB-AUXILIAIRE">
    <xs:complexType>
      <xs:attribute name="AUXIL" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="PARTICIPE-PASSE" type="xs:NCName"/>
  <xs:element name="RENVOI-VRB">
    <xs:complexType>
      <xs:choice>
        <xs:element ref="lod:VARIANTE-HOMOSEME"/>
        <xs:element ref="lod:VARIANTE-ORTHOGRAPHIQUE"/>
      </xs:choice>
      <xs:attribute name="REF-ID-ITEM-ADRESSE" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="TRAITEMENT-LING-VRB">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lod:REFLEX-PRONOM"/>
        <xs:element ref="lod:EMPLOI-IMPERS"/>
        <xs:element ref="lod:TRADUCTION-DOMINANTE"/>
        <xs:element maxOccurs="unbounded" ref="lod:UNITE-TRAD"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="REFLEX-PRONOM">
    <xs:complexType>
      <xs:attribute name="REFPRON" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="EMPLOI-IMPERS">
    <xs:complexType>
      <xs:attribute name="EMPLOI-IMPERS" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="RESEAU-NON-SEMANTIQUE">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="lod:CITOYEN"/>
        <xs:element minOccurs="0" ref="lod:CITOYENNE"/>
        <xs:element minOccurs="0" ref="lod:PAYS"/>
        <xs:element minOccurs="0" ref="lod:ADJ-CORRESP"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="CITOYEN">
    <xs:complexType>
      <xs:attribute name="REF-ID-ITEM-ADRESSE" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="CITOYENNE">
    <xs:complexType>
      <xs:attribute name="REF-ID-ITEM-ADRESSE" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="PAYS">
    <xs:complexType>
      <xs:attribute name="REF-ID-ITEM-ADRESSE" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="ADJ-CORRESP">
    <xs:complexType>
      <xs:attribute name="REF-ID-ITEM-ADRESSE" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="tag">
    <xs:complexType mixed="true">
      <xs:attribute name="typ" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="AUDIO">
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="xs:base64Binary">
          <xs:attribute name="FORMAT" use="required" form="qualified" type="xs:NCName"/>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="FLX-ADJ">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lod:GRONDFORMEN"/>
        <xs:element ref="lod:DEKL"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="GRONDFORMEN">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lod:POSITIV"/>
        <xs:element minOccurs="0" ref="lod:KOMPARATIV"/>
        <xs:element minOccurs="0" ref="lod:SUPERLATIV"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="DEKL">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lod:POSITIV"/>
        <xs:element minOccurs="0" ref="lod:KOMPARATIV"/>
        <xs:element minOccurs="0" ref="lod:SUPERLATIV"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="FLX-VRB">
    <xs:complexType>
      <xs:sequence>
